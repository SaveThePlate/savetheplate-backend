# Database
DATABASE_URL="postgresql://user:password@localhost:5432/savetheplate"
DB_NAME=savetheplate
DB_USER=savetheplate_user
DB_PASSWORD=your_secure_password

# JWT
JWT_SECRET=your_jwt_secret_here

# Environment
NODE_ENV=development
PORT=3001

# Redis
REDIS_URL=redis://localhost:6379
# Optional: Use these if not using REDIS_URL
# REDIS_HOST=localhost
# REDIS_PORT=6379
# REDIS_PASSWORD=

# Frontend URLs (for CORS and redirects)
FRONT_URL=http://localhost:3000
FRONTEND_URL=http://localhost:3000
BACKEND_URL=http://localhost:3001

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:3000
CORS_ALLOWED_ORIGIN_SUFFIXES=
CORS_ALLOW_LOCALHOST=true

# Email Service (Resend)
RESEND_TOKEN=your_resend_api_key_here
RESEND_DOMAIN=savetheplate.tn

# Google OAuth (Optional)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Facebook OAuth (Optional)
FACEBOOK_APP_ID=your_facebook_app_id
FACEBOOK_APP_SECRET=your_facebook_app_secret

# Google Maps API
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Admin Configuration
ADMIN_EMAILS=savetheplatetunisia@gmail.com,admin@example.com

# API & Scripts
API_URL=http://localhost:3001
AUTH_TOKEN=your_auth_token_for_scripts

# Swagger Documentation (Production)
ENABLE_SWAGGER=false
SWAGGER_USER=admin
SWAGGER_PASSWORD=change_this_password

# Development Only
ALLOW_UNSAFE_EVAL=false
