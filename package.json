{
  "name": "nest-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:clean": "rm -rf dist tsconfig.build.tsbuildinfo && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "start:staging": "ENV_FILE=.env.staging NODE_ENV=production node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "install-githooks": "git config core.hooksPath .githooks || true",
    "check-orphaned-images": "ts-node src/scripts/check-orphaned-images.ts"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/common": "^11.1.11",
    "@nestjs/core": "^11.1.11",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.0",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/platform-socket.io": "^11.1.11",
    "@nestjs/schedule": "^5.0.0",
    "@nestjs/swagger": "^8.0.0",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/websockets": "^11.1.11",
    "@prisma/client": "^5.18.0",
    "prisma": "^5.18.0",
    "@react-email/components": "^1.0.1",
    "@react-email/render": "^2.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "blurhash": "^2.0.5",
    "cache-manager": "^7.2.7",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.1",
    "multer": "^2.0.2",
    "nestjs-prisma": "^0.23.0",
    "nestjs-resend": "^0.0.3",
    "node-cron": "^3.0.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-magic-login": "^1.2.2",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.2.0",
    "resend": "^3.4.0",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.11",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/passport-local": "^1.0.38",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  },
  "overrides": {
    "form-data": "^4.0.4",
    "multer": "^2.0.2",
    "validator": "^13.15.23",
    "jws": "^4.0.1",
    "glob": "^13.0.0",
    "@babel/helpers": "^7.26.10",
    "js-yaml": "^4.1.1",
    "prismjs": "^1.30.0",
    "formidable": "^3.5.4",
    "tmp": "^0.2.5",
    "brace-expansion": "^2.0.2",
    "qs": "^6.14.1",
    "body-parser": "^1.20.3",
    "@react-email/components": {
      "react": "^19.2.1",
      "react-dom": "^19.2.1"
    }
  }
}
